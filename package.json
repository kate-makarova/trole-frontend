{
  "name": "trole-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start_dev": "ng serve  --configuration development",
    "start_test": "ng serve  --configuration testing",
    "build": "ng build",
    "build:prod": "ng build --configuration production --aot",
    "build_test": "ng build --configuration testing",
    "watch": "ng build --watch --configuration development",
    "watch_test": "ng build --watch --configuration testing",
    "analyze": "ng build --configuration production --stats-json && npx webpack-bundle-analyzer dist/trole-frontend/stats.json",
    "test": "ng test",
    "update_editor": "npm update sceditor-angular",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "ng serve --configuration development & cypress run",
    "e2e:visual": "cypress run --env type=actual",
    "e2e:visual:base": "cypress run --env type=base",
    "e2e:visual:compare": "cypress run --env type=comparison"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@ngrx/store": "^19.0.0",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^7.0.0",
    "angular-ng-autocomplete": "^2.0.12",
    "ng2-adsense": "^14.0.0",
    "rxjs": "~7.8.0",
    "sceditor-angular": "git+git@github.com:kate-makarova/sceditor-angular.git",
    "socket.io-client": "^4.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.0",
    "@angular/localize": "^19.0.5",
    "@cypress/schematic": "^2.5.1",
    "@types/jasmine": "~5.1.0",
    "cypress": "^13.6.6",
    "cypress-visual-regression": "^3.0.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ngx-build-plus": "^17.0.0",
    "typescript": "~5.6.2",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
